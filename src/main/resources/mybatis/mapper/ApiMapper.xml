<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hnisi.wx.server.filter.dao.ApiDAO">
    <select id="querySecurityByUrl" resultType="cn.hnisi.wx.server.filter.model.Api">
        SELECT
          *
        FROM
          t_api
        WHERE
          apiurl = #{url}
    </select>

</mapper>